<h1>Hi <%= user.name %></h1>
<h2>My Routines</h2>
<% if (routines == null || routines.length == 0) {%>
  <h3>No Routines</h3>
<%} else {%>
  <table>
  <% for (var i = 0; i < routines.length; i++) { %>
      <tr>
        <td>
          <a href='/myroutine?id=<%= routines[i].id %>'><%= routines[i].name %></a>
        </td>
      </tr>
  <%}%>
  </table>
<%}%>
<br><br>
<a href='/'>Back</a>
